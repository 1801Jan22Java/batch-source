<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Project_1</display-name>
  <servlet>
    <display-name>Login</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.revature.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EmployeeInfo</display-name>
    <servlet-name>EmployeeServlet</servlet-name>
    <servlet-class>com.revature.servlets.EmployeeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmployeeServlet</servlet-name>
    <url-pattern>/employee</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PendingRequestInfo</display-name>
    <servlet-name>ReimbursementRequestServlet</servlet-name>
    <servlet-class>com.revature.servlets.ReimbursementRequestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReimbursementRequestServlet</servlet-name>
    <url-pattern>/pendingRequest</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ProcessedRequests</display-name>
    <servlet-name>ProcessedRequestServlet</servlet-name>
    <servlet-class>com.revature.servlets.ProcessedRequestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProcessedRequestServlet</servlet-name>
    <url-pattern>/processedRequests</url-pattern>
  </servlet-mapping>
</web-app>