<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>EmployeeReimbursement</display-name>
  <servlet>
    <servlet-name>admin</servlet-name>
    <servlet-class>com.revature.servlets.MasterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>admin</servlet-name>
    <url-pattern>/admin/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.revature.servlets.LogInServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>error</servlet-name>
    <servlet-class>com.revature.servlets.ErrorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>error</servlet-name>
    <url-pattern>/error</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>com.revature.servlets.LogOutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>404</error-code>
    <location>/error</location>
  </error-page>
  
  <servlet>
    <servlet-name>session</servlet-name>
    <servlet-class>com.revature.servlets.SessionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>session</servlet-name>
    <url-pattern>/session</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>com.revature.servlets.HomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>checklogin</servlet-name>
    <servlet-class>com.revature.servlets.CheckLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>checklogin</servlet-name>
    <url-pattern>/checklogin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>profile</servlet-name>
    <servlet-class>com.revature.servlets.ProfileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>profile</servlet-name>
    <url-pattern>/profile</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>viewpending</servlet-name>
    <servlet-class>com.revature.servlets.ViewPendingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>viewpending</servlet-name>
    <url-pattern>/viewpending</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>viewprofile</servlet-name>
    <servlet-class>com.revature.servlets.ViewProfileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>viewprofile</servlet-name>
    <url-pattern>/viewprofile</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>viewresolved</servlet-name>
    <servlet-class>com.revature.servlets.ViewResolvedServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>viewresolved</servlet-name>
    <url-pattern>/viewresolved</url-pattern>
  </servlet-mapping>
</web-app>