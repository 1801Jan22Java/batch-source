<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<display-name>login</display-name>
  	<servlet-class>com.revature.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>employeeHome</servlet-name>
  	<display-name>employeeHome</display-name>
  	<servlet-class>com.revature.servlet.EmployeeHomeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>newRequest</servlet-name>
  	<display-name>newRequest</display-name>
  	<servlet-class>com.revature.servlet.NewRequestServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>submitRequest</servlet-name>
  	<display-name>submitRequest</display-name>
  	<servlet-class>com.revature.servlet.SubmitRequestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>employeeHome</servlet-name>
  	<url-pattern>/employeeHome</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>newRequest</servlet-name>
  	<url-pattern>/newRequest</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>submitRequest</servlet-name>
  	<url-pattern>/submitRequest</url-pattern>
  </servlet-mapping>
</web-app>